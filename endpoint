javascript:(function(){var scripts=document.getElementsByTagName("script"),regex=/(?<=(\"|\%27|\`))\/[a-zA-Z0-9_?&=\/\-\#\.]*(?=(\"|\'|\%60))/g,results=new%20Set,timeoutDelay=3000;for(var%20i=0;i<scripts.length;i++){var%20src=scripts[i].src;if(src)fetch(src).then(response=>response.text()).then(content=>{let%20matches=content.matchAll(regex);for(let%20match%20of%20matches)results.add(match[0])}).catch(error=>console.error("Error%20fetching%20script:",src,error))}var%20pageContent=document.documentElement.outerHTML,matches=pageContent.matchAll(regex);for(const%20match%20of%20matches)results.add(match[0]);function%20showResults(){const%20modal=document.createElement("div");modal.style.position="fixed",modal.style.top="10%",modal.style.left="50%",modal.style.transform="translate(-50%,-10%)",modal.style.background="#333",modal.style.color="#fff",modal.style.border="1px%20solid%20#444",modal.style.boxShadow="0%204px%208px%20rgba(0,%200,%200,%200.2)",modal.style.zIndex=9999,modal.style.padding="20px",modal.style.maxHeight="80%",modal.style.overflowY="auto",modal.style.fontFamily="Arial,%20sans-serif",modal.style.fontSize="14px",modal.style.borderRadius="8px";const%20header=document.createElement("h3");header.innerText="Extracted%20URLs/Paths",header.style.marginTop="0",header.style.color="#ffa500";const%20list=document.createElement("ul");results.forEach(item=>{const%20listItem=document.createElement("li");listItem.textContent=item,listItem.style.marginBottom="5px",list.appendChild(listItem)});const%20closeButton=document.createElement("button");closeButton.innerText="Close",closeButton.style.marginTop="10px",closeButton.style.padding="5px%2010px",closeButton.style.background="#ffa500",closeButton.style.color="#000",closeButton.style.border="none",closeButton.style.cursor="pointer",closeButton.style.borderRadius="4px",closeButton.onclick=()=>document.body.removeChild(modal);const%20downloadButton=document.createElement("button");downloadButton.innerText="Download%20Results",downloadButton.style.marginLeft="10px",downloadButton.style.padding="5px%2010px",downloadButton.style.background="#4caf50",downloadButton.style.color="#fff",downloadButton.style.border="none",downloadButton.style.cursor="pointer",downloadButton.style.borderRadius="4px",downloadButton.onclick=()=>{const%20blob=new%20Blob([Array.from(results).join("\n")],{type:"text/plain"}),link=document.createElement("a");link.href=URL.createObjectURL(blob),link.download="extracted_paths.txt",link.click()};modal.appendChild(header),modal.appendChild(list),modal.appendChild(closeButton),modal.appendChild(downloadButton),document.body.appendChild(modal)}setTimeout(showResults,timeoutDelay)})();